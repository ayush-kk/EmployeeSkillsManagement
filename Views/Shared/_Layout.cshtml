<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Employee Skills Management</title>
    <link rel="icon" type="image/png" href="~/images/favicon.png?v=@DateTime.Now.Ticks">
    @* <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script> *@
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        asp-append-version="true">
    <link href="~/DataTables/datatables.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/darkmode.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
        asp-append-version="true">
</head>

<body>
    <div class="wrapper">
        @if (User.Identity.IsAuthenticated)
        {
            <!-- Sidebar -->
            <aside id="sidebar">
                <div class="h-100">
                    <div class="sidebar-logo">
                        <h2 style="color: #fff; display: flex;">Employee Skills Management</h2>
                    </div>
                    <div class="navigation">
                        <ul class="sidebar-nav" id="sidebar-decoration">
                            <li
                                class="sidebar-item @(ViewContext.RouteData.Values["Controller"].ToString() == "Home" && ViewContext.RouteData.Values["Action"].ToString() == "Index" ? "active" : "")">
                                <a asp-action="Index" asp-controller="Home" class="sidebar-link">
                                    <i class="fas fa-home"></i>
                                    Home
                                </a>
                            </li>
                            <li
                                class="sidebar-item @(ViewContext.RouteData.Values["Controller"].ToString() == "Employee" && ViewContext.RouteData.Values["Action"].ToString() == "Index" ? "active" : "")">
                                <a asp-action="Index" asp-controller="Employee" class="sidebar-link collapsed">
                                    <i class="fas fa-users"></i>
                                    Manage Employees
                                </a>
                            </li>
                            <li
                                class="sidebar-item @(ViewContext.RouteData.Values["Controller"].ToString() == "Skill" && ViewContext.RouteData.Values["Action"].ToString() == "Index" ? "active" : "")">
                                <a asp-action="Index" asp-controller="Skill" class="sidebar-link collapsed">
                                    <i class="fas fa-graduation-cap"></i>
                                    Manage Skills
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link collapsed" data-bs-toggle="modal" data-bs-target="#logoutModal"
                                    style="color: #fff;">
                                    <i class="fas fa-sign-out-alt"></i>
                                    @Html.AntiForgeryToken()
                                    Logout
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </aside>
        }
        <!-- Main Component -->
        <div class="main">

            @if (User.Identity.IsAuthenticated)
            {
                <nav class="navbar navbar-expand px-3 border-bottom" style="background-color: #272936;">
                    <!-- Button for sidebar toggle -->
                    <button class="btn" type="button" style="background-color: #fff;">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <span>
                        <img id="company-logo" alt="No Image" class="company-logo-image"
                            src="data:image/png;base64,"
                            width="223" height="46" style=" padding-top: 5px;">
                    </span>

                </nav>
            }
            <main class="content px-3 py-2">
                <div class="container-fluid mb-3">
                    <div id="notification-toast" class="toast align-items-center position-fixed top-0 end-0 m-4"
                        role="alert" aria-live="assertive" aria-atomic="true" data-bs-autohide="false"
                        style="z-index: 1050;">

                        <div class="d-flex">
                            <div class="toast-body">
                                <span id="notification-message"></span>
                            </div>
                            <button type="button" class="btn-close me-2 m-auto" data-bs-dismiss="toast"
                                aria-label="Close"></button>
                        </div>

                    </div>

                    @RenderBody()
                </div>
            </main>
        </div>
    </div>

    <div class="modal fade" id="logoutModal" tabindex="-1" aria-labelledby="logoutModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="logoutModalLabel">Logout Confirmation</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <p>Are you sure you want to log out?</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" onclick="logout()">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <script src="script.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/DataTables/datatables.min.js"></script>
    <script src="~/js/datatable.js"></script>
    <script src="~/js/darkmode.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://kit.fontawesome.com/ae360af17e.js" crossorigin="anonymous" asp-append-version="true"></script>

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var toastrMessage = '@(TempData["ToastrMessage"] as string)';
            var toastrType = '@(TempData["ToastrType"] as string)'; // Add a TempData entry for ToastrType
            if (toastrMessage) {
                // Display Bootstrap Toast
                var toast = new bootstrap.Toast(document.getElementById('notification-toast'));
                // Set background color based on toastrType
                var bgColor = toastrType === "success" ? "#28a745" : toastrType === "error" ? "#dc3545" : "";
                // Apply background color to the toast
                document.getElementById('notification-toast').style.backgroundColor = bgColor;
                // Set the message and show the toast
                document.getElementById('notification-message').innerText = toastrMessage;
                toast.show();
            }
        });
    </script>



    <script>
        function logout() {
            $.post('@Url.Action("Logout", "Admin")', {
                __RequestVerificationToken: $('input[name="__RequestVerificationToken"]').val()
            })
                .done(function () {
                    window.location.href = '@Url.Action("Login", "Admin")';
                })
                .fail(function () {
                    alert('Logout failed');
                });
        }
    </script>

    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const navButtons = document.querySelectorAll('.nav-button');
            const navContentHighlight = document.getElementById('nav-content-highlight');
            navButtons.forEach(function (button, index) {
                button.addEventListener('click', function () {
                    // Remove 'active' class from all buttons
                    navButtons.forEach(function (btn) {
                        btn.classList.remove('active');
                    });
                    // Add 'active' class to the clicked button
                    button.classList.add('active');
                    // Adjust top position of the highlight based on the index
                    const topPosition = 16 + index * 54;
                    navContentHighlight.style.top = `${topPosition}px`;
                });
            });
        });

    </Script>

    <script>
        const toggler = document.querySelector(".btn");
        toggler.addEventListener("click", function () {
            document.querySelector("#sidebar").classList.toggle("collapsed");
        });
    </script>
    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>
